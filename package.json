{
  "private": true,
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "browser-sync": "^2.7.12",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^1.1.1",
    "gulp-file-include": "^0.13.7",
    "gulp-inject": "^3.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-svgmin": "^1.2.1",
    "gulp-svgstore": "^5.0.5",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6"
  },
  "dependencies": {
    "bootstrap": "git+https://github.com/twbs/bootstrap.git#v4-dev",
    "jquery": "^2.2.0",
    "leaflet": "^0.7.7"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-console": 0,
      "no-unused-vars": 0
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "globals": {
      "Dom": false,
      "Map": false,
      "Router": false,
      "Sparql": false,
      "$": false,
      "L": false
    },
    "extends": "eslint:recommended"
  }
}
